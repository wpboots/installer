{
    "name": "boots/installer",
    "type": "composer-plugin",
    "description": "Composer installer for the boots framework.",
    "keywords": ["boots","extension","installer","wordpress"],
    "homepage": "http://github.com/wpboots/installer",
    "license": "MIT",
    "authors": [
        {
            "name": "M. Kamal Khan",
            "email": "shout@bhittani.com",
            "homepage": "http://bhittani.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Boots\\Installer\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "bhittani/php-parser": "dev-master",
        "composer-plugin-api": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "extra": {
        "class": "Boots\\Installer\\Plugin"
    }
}
